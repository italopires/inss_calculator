<div class="box-body">
  <%= f.input :name, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :cpf, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :birthdate, as: :string, input_html: { class: 'form-control datepicker',
    value: (l(f.object.birthdate) if f.object.birthdate.present?) },
    wrapper_html: { class: 'form-group date' } %>
  <%= f.input :public_place, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :address_number, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :neighborhood, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :cep, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :personal_phone, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :reference_phone, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :salary, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' } %>
  <%= f.input :inss_discount, input_html: { class: 'form-control', autocomplete: 'nope' },
    wrapper_html: { class: 'form-group' }, readonly: true %>
  <%= f.association :state, as: :select, collection: @states,
    input_html: { class: 'form-control' }, wrapper_html: { class: 'form-group' } %>
  <%# binding.pry %>
  <%# f.association :city, as: :select, collection: @cities, selected: f.object.city_id, include_blank: false,
    input_html: { class: 'form-control' }, wrapper_html: { class: 'form-group' } %>

  <%= f.input :city_id, collection: @cities, include_blank: true,
              selected: f.object.city_id, input_html: { class: 'form-control', data: { selected: f.object.city_id } }, wrapper_html: { class: 'form-group' } %>
</div>

<div class="box-footer">
  <%= render 'layouts/common/buttons/back', link: proponents_path %>
  <%= render 'layouts/common/buttons/submit' %>
</div>