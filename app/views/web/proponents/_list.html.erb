<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Listagem</h3>
      </div>  <!-- /.box-header -->
      <div class="box-body">
        <table class="table table-bordered table-hover">
          <thead>
            <tr>
              <th><%= Proponent.human_attribute_name(:id) %></th>
              <th><%= Proponent.human_attribute_name(:name) %></th>
              <th><%= Proponent.human_attribute_name(:cpf) %></th>
              <th><%= Proponent.human_attribute_name(:birthdate) %></th>
              <th><%= Proponent.human_attribute_name(:public_place) %></th>
              <th><%= Proponent.human_attribute_name(:address_number) %></th>
              <th><%= Proponent.human_attribute_name(:neighborhood) %></th>
              <th><%= Proponent.human_attribute_name(:cep) %></th>
              <th><%= Proponent.human_attribute_name(:personal_phone) %></th>
              <th><%= Proponent.human_attribute_name(:reference_phone) %></th>
              <th><%= Proponent.human_attribute_name(:salary) %></th>
              <th><%= Proponent.human_attribute_name(:inss_discount) %></th>
              <th><%= t('core.links.actions.title') %></th>
            </tr>
          </thead>
          <tbody>
            <% @objects.each do |object| %>
              <tr>
                <td><%= object.id %></td>
                <td><%= object.name %></td>
                <td><%= object.cpf %></td>
                <td><%= l object.birthdate if object.birthdate.present? %></td>
                <td><%= object.public_place %></td>
                <td><%= object.address_number %></td>
                <td><%= object.neighborhood %></td>
                <td><%= object.cep %></td>
                <td><%= object.personal_phone %></td>
                <td><%= object.reference_phone %></td>
                <td><%= number_to_currency object.salary %></td>
                <td><%= number_to_currency object.inss_discount %></td>
                <td>
                  <%= render 'layouts/common/icon_links/edit', link: edit_proponent_path(object) %>
                  <%= render 'layouts/common/icon_links/destroy', link: proponent_path(object) %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div> <!-- /.box-body -->
    </div> <!-- /.box -->
  </div>
</div>

